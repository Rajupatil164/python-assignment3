num = float(input("ENTER A NUMBER: "))

import math

if num < 0:
    print("Error: Cannot compute square for negative number.")
else:
    square_root = math.sqrt(num)
    print(f"The square root of {num} is: {square_root}")

if num < 0:
    print("Error: Cannot compute logarithm for negative number.")
else:
    logarithm = math.log(num)
    print(f"The natural logarithm of {num} is: {logarithm}")

sine = math.sin(num)
print(f"The sine of {num} is: {sine}")
